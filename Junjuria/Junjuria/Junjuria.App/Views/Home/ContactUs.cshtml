@{
    ViewData["Title"] = "Contact Us:";
    var testObject = new ResponseMessage("Prakash", "Indiec", "SomeComment");
}
<div class="d-flex justify-content-center pt-2">
    <h1 class="m-0"><i class="fas fa-search"></i> &nbsp; @ViewData["Title"]</h1>
</div>
<hr class="hr-jun w-50 my-2" />
<center>
    <div></div>
    <div class="w-50">
        <div class="px-4 rounded" id="chatContainer">
            @for (int i = 0; i < 10; i++)
            {
                <partial name="_ChatMessageUserPartial" model="@testObject" />
            }
        </div>
        <div class="row justify-content-between mt-5">
            <div class="col-2">
                <label class="form-inline h3 text-left" for="MyMessage">Write message:</label>
            </div>
            <div class="col-8">
                <textarea id="MyMessage" class="form-inline rounded" rows="3" cols="70" placeholder="Write message..."></textarea>
            </div>
            <div class="col-2">
                <button class="btn btn-primary btn-lg form-inline" id="SendMyMessage">Send</button>
            </div>
        </div>
    </div>



</center>